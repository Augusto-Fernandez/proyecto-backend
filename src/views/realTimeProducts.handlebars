<div>
    <h1>Products</h1>
    {{#each productsArray}}
        <div>
            <p>{{this.title}}</p>
            <p>{{this.thumbnail}}</p>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>
            <p>{{this.code}}</p>
            <p>{{this.stock}}</p>
        </div>
    {{/each}}
</div>